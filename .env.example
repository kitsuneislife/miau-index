# Example .env file for @kitsuneislife/miau-index
# Copy this to .env and fill in your API credentials

# MyAnimeList API
MAL_CLIENT_ID=your_mal_client_id
MAL_CLIENT_SECRET=your_mal_client_secret

# AniList API
ANILIST_CLIENT_ID=your_anilist_client_id
ANILIST_CLIENT_SECRET=your_anilist_client_secret

# Kitsu API
KITSU_API_KEY=your_kitsu_api_key

# Application Settings
NODE_ENV=development
LOG_LEVEL=info

# Repository Settings
REPOSITORY_TYPE=memory
# DATABASE_URL=postgresql://user:password@localhost:5432/miauindex

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL=3600

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_RPM=60
