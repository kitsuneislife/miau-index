{
  "name": "@kitsuneislife/miau-index",
  "version": "1.0.0",
  "description": "A powerful TypeScript anime indexer that aggregates and unifies anime data from multiple sources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "anime",
    "indexer",
    "myanimelist",
    "anilist",
    "kitsu",
    "typescript"
  ],
  "author": "kitsuneislife",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "optionalDependencies": {
    "@kitsuneislife/nyaa": "^1.0.0"
  },
  "peerDependencies": {
    "@kitsuneislife/nyaa": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@kitsuneislife/nyaa": {
      "optional": true
    }
  }
}
